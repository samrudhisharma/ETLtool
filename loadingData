    private void jButton3MouseClicked(java.awt.event.MouseEventevt) {
        // TODO add your handling code here:
        try
        {
            String dest = f.getCanonicalPath()+File.separator+"dest.txt";
            File fin1 = new File(dest);
            FileInputStream fis1 = new FileInputStream(fin1);
            BufferedReader in1 = new BufferedReader(new InputStreamReader(fis1));
            
            String input = "";
            input = in1.readLine();
            StringTokenizer line = new StringTokenizer(input);
            no_attr = line.countTokens()-1;
            fileArray = new String[28][no_attr+1];
            
            for(int i=0;i&lt;=no_attr;i++)
            {
                fileArray[0][i] = line.nextToken();
            }
            
            while(true)
            {
                finalOutput = in1.readLine();
                if(finalOutput == null) break;
                line = new StringTokenizer(finalOutput);
                no_row++;
                for(int i=0;i&lt;=no_attr;i++)
                fileArray[no_row][i] = line.nextToken();
                
            }
            
            
            int c=0;
            for(int i=0;i&lt;28;i++)
            {
                for(int j=0;j&lt;=no_attr;j++)
                {
                    if(fileArray[i][1].compareTo("Allergy")==0||fileArray[i][1].compareTo("Cancer")==0||fileArray[i][1].compareTo("Cold")==0||fileArray[i][1].compareTo("Acidity")==0)
                    {
                    }
                    else{
                        c++;
                    }
                }
            }
            
            if(c&gt;0)
            {
                JOptionPane.showMessageDialog(null,"Some symptom names are wrong!They will be transformed!!");
            }
            
            for(int i=0;i&lt;28;i++)
            {
                for(int j=0;j&lt;=no_attr;j++)
                {
                    if(fileArray[i][1].compareTo("Allergy")==0||fileArray[i][1].compareTo("Cancer")==0||fileArray[i][1].compareTo("Cold")==0||fileArray[i][1].compareTo("Acidity")==0)
                    {
                    }
                    else
                    {
                        String str = fileArray[i][1].substring(0, 1) ;
                        intlen = fileArray[i][1].length();
                        
                        String str2 = fileArray[i][1].substring(len-1,len);
                        String str3 = fileArray[i][1].substring(len-2,len);
                        if(str.compareTo("C")==0&&str2.compareTo("r")==0)
                        fileArray[i][1]="Cancer";
                        if(str.compareTo("C")==0&&str2.compareTo("d")==0)
                        fileArray[i][1]="Cold";
                        if(str.compareTo("A")==0&&str3.compareTo("gy")==0)
                        fileArray[i][1]="Allergy";
                        if(str.compareTo("A")==0&&str3.compareTo("ty")==0)
                        fileArray[i][1]="Acidity";
                    }
                }
            }
            String fin = "";
            for(int i=0;i&lt;28;i++)
            {
                for(int j=0;j&lt;=no_attr;j++)
                {
                    fin+= fileArray[i][j]+"          ";
                }
                fin+="\n";
            }
            
            
            jTextArea1.setText(fin);
            
        }
        catch(Exception e)
        {}
    }
    
    private void jButton2MouseClicked(java.awt.event.MouseEventevt) {
        // TODO add your handling code here:
        try
        {
            Connection con = null;
            PreparedStatementstmt = null;
            PreparedStatement stmt1 = null;
            Class.forName("sun.jdbc.odbc.JdbcOdbcDriver") ;
            con = DriverManager.getConnection("jdbc:odbc:finda");
            //System.out.println("cona");
            for(int i=0;i&lt;28;i++)
            {
                for(int j=0;j&lt;=no_attr;j++)
                {
                    System.out.print(fileArray[i][j]+" ");
                }
                System.out.println();
            }
            
            String s1 = "delete * from Table1";
            stmt1 = con.prepareStatement(s1);
            stmt1.execute();
            stmt1.close();
            for(int i=0;i&lt;28;i++)
            {
                String s = "Insert into Table1 (medN,medS,medP,medD,medC) values (?,?,?,?,?)";
                stmt = con.prepareStatement(s);
                String value1 = fileArray[i][0];
                String value2 = fileArray[i][1];
                String value3 = fileArray[i][2];
                String value4 = fileArray[i][3];
                String value5 = fileArray[i][4];
                stmt.setString(1,value1);
                stmt.setString(2,value2);
                stmt.setString(3,value3);
                stmt.setString(4,value4);
                stmt.setString(5,value5);
                
                stmt.execute();
                stmt.close();
            }
            
            
            
            
            con.close();
            
        }
        catch(Exception e)
        {
            System.err.println("Exception: "+e.getMessage());
        }
        
        
    }
    
