    private void jButton1MouseClicked(java.awt.event.MouseEventevt) {
        // TODO add your handling code here:
        
        try
        {
            
            
            File f = new File(".");
            File yourFile = new File("dest.txt");
            yourFile.delete();
            String dest = f.getCanonicalPath()+File.separator+"dest.txt";
            FileWriterfstream = new FileWriter(dest, true);
            BufferedWriter out = new BufferedWriter(fstream);
            
            //The code below will extract data from an access sheet
            Connection con = null;
            ResultSetrs = null;
            Connection con1 = null;
            
            String pass ="";
            Class.forName("sun.jdbc.odbc.JdbcOdbcDriver") ;
            con = DriverManager.getConnection("jdbc:odbc:medicine");
            //String source2 = "JDBC-ODBC driver loaded ok.";
            //JOptionPane.showMessageDialog(null,source2);
            Statement stmt = con.createStatement();
            String s = "select * from medicine;";
            rs = stmt.executeQuery(s);
            String str ="";
            while(rs.next())
            {
                pass="";
                for(int i =1 ; i&lt;=5;i++)
                {
                    
                    pass += rs.getString(i)+" ";
                    // out.write(pass);
                    //out.newLine();
                    // if(i!=4)
                    // pass+=",";*/
                    //System.out.print(rs.getString(i));
                }
                // System.out.println();
                out.write(pass);
                out.newLine();
                str += pass + "\n";
                //System.out.println(pass);
            }
            // System.out.println(str);
            con.close();
            
            //The code block below is used to extract data from an excel sheet
            Connection con2 = DriverManager.getConnection("jdbc:odbc:mede");
            
            Statement st2 = con2.createStatement();
            ResultSet rs1 = st2.executeQuery("Select * from [Sheet1$]");
            
            ResultSetMetaDatarsmd = rs1.getMetaData();
            intnumberOfColumns = rsmd.getColumnCount();
            
            String str1 ="";
            String columnValue = "";
            while (rs1.next()) {
                
                columnValue ="";
                for (int i = 1; i &lt;= numberOfColumns; i++) {
                    
                    columnValue += rs1.getString(i)+" ";
                    // System.out.print(columnValue);
                    
                }
                out.write(columnValue);
                out.newLine();
                str1 += columnValue + "\n";
                
            }
            
            // System.out.println(str1);
            
            st2.close();
            con2.close();
            //The code below is used to extract data from a file (.txt)
            // File f = new File(".");
            String source1 = f.getCanonicalPath() + File.separator+ "medicine.txt";
            // JOptionPane.showMessageDialog(null,source1);
            
            //  JOptionPane.showMessageDialog(null,dest22);
            File fin1 = new File(source1);
            FileInputStream fis1 = new FileInputStream(fin1);
            BufferedReader in1 = new BufferedReader(new InputStreamReader(fis1));
            
            
            String aLine = null;
            while((aLine=in1.readLine())!=null)
            {
                out.write(aLine);
                finalOutput+=aLine+"\n";
                out.newLine();
            }
            finalOutput+=str+str1;
            
            jTextArea1.setText(finalOutput);
            
            
            
            in1.close();
            out.close();
            
        }
        catch(Exception e)
        {
            System.out.println(e.getMessage());
        }
    }
    
